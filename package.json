{
    "name": "@purinton/modify",
    "type": "module",
    "version": "1.0.0",
    "description": "A modern, modular Discord moderation and utility bot with OpenAI-powered content moderation, multi-language support, and easy customization.",
    "main": "modify.mjs",
    "scripts": {
        "start": "node modify.mjs",
        "test": "npx --node-options=\"--experimental-vm-modules --no-warnings\" jest --detectOpenHandles --silent"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/purinton/modify.git"
    },
    "keywords": [
        "discord",
        "discord-bot",
        "moderation",
        "openai",
        "ai-moderation",
        "localization",
        "multi-language",
        "nodejs",
        "bot",
        "purinton",
        "utility",
        "commands",
        "events"
    ],
    "author": "Russell Purinton <russell@purinton.us>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/purinton/modify/issues"
    },
    "homepage": "https://github.com/purinton/modify#readme",
    "devDependencies": {
        "jest": "^30.0.2"
    },
    "jest": {
        "testEnvironment": "node",
        "testMatch": [
            "**/?(*.)+(spec|test).[jt]s?(x)",
            "**/?(*.)+(spec|test).mjs",
            "**/?(*.)+(spec|test).cjs"
        ]
    },
    "dependencies": {
        "@purinton/common": "^1.0.16",
        "@purinton/discord": "^1.0.11",
        "@purinton/mysql": "^1.0.9",
        "@purinton/openai": "^1.0.1",
        "dotenv": "^16.5.0"
    }
}
